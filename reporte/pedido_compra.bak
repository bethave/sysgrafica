    cpd.cantidad,
    cpd.precio,
    cpd.iva,
    i.item_descrip
   FROM compra_pedido cp, compra_pedido_detalle cpd, sucursales s, items i
   where s.id_suc = cp.id_suc
   and i.id_item = cpd.id_item and cpd.id_ped=1 and cp.id_ped=1;
