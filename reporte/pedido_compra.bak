	<queryString><![CDATA[ SELECT cp.id_ped,
    cp.id_suc,
    cp.ped_fecha,
    cp.id_usu,
    cp.estado,
    s.suc_nombre,
    s.suc_direccion,
    s.suc_celular,
    to_char(cp.ped_fecha::timestamp with time zone, 'DD/MM/YYYY'::text) AS fecha,
    cpd.id_ped,
